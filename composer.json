{
  "name": "fuguevit/eloquent-commentable",
  "type": "library",
  "description": "Implementation of commentable/remarkable traits for Laravel5 Eloquent models.",
  "license": "MIT",
  "homepage": "http://www.fuguevit.com",
  "keywords": ["polymorphic","laravel","comment","remark","eloquent"],
  "authors": [
    {
      "name": "Frank Wang",
      "email": "wangxuan@188.com"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "illuminate/config": "~5.2.0|~5.3.0",
    "illuminate/support": "~5.2.0|~5.3.0",
    "illuminate/database": "~5.2.0|~5.3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*",
    "orchestra/testbench": "3.2.x-dev",
    "orchestra/database": "3.2.x-dev"
  },
  "autoload": {
    "psr-4": {
      "Fuguevit\\Commentable\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fuguevit\\Commentable\\Test\\": "tests/"
    }
  },
  "extra": {
    "component": "package",
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}